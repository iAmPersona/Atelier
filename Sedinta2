listaInitiala = [7,8,9,2,3,1,4,10,5,6]

# sortare ascendenta
listaAscendenta = sorted(listaInitiala)
print(listaAscendenta)

# sortare descendenta
listaDescendenta = sorted(listaInitiala, reverse=True)
print(listaDescendenta)

# numere de pe indice par/impar
print(listaInitiala[0:10:2])
print(listaInitiala[1:10:2])

# multiplii de 3
for i in listaInitiala:
    if i %  3 == 0:
        print(i,' ', end ='')

